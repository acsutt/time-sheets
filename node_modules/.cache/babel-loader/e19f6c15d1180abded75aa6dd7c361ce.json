                    this.Users = res.data;\r\n                })\r\n                .catch(error => {\r\n                    console.log(error);\r\n                });\r\n        },\r\n        methods: {\r\n            deleteActivity(id) {\r\n                let apiURL = `http://localhost:4000/user-api/delete-user/${id}`;\r\n                let indexOfArrayItem = this.Users.findIndex(i => i._id === id);\r\n\r\n                if (window.confirm('Do you really want to delete?')) {\r\n                    axios\r\n                        .delete(apiURL)\r\n                        .then(() => {\r\n                            this.Users.splice(indexOfArrayItem, 1);\r\n                        })\r\n                        .catch(error => {\r\n                            console.log(error);\r\n                        });\r\n                }\r\n            },\r\n        },\r\n    };\r\n</script>\r\n\r\n<style>\r\n    .btn-success {\r\n        margin-right: 10px;\r\n    }\r\n</style>"],"sourceRoot":"src/components"}]}