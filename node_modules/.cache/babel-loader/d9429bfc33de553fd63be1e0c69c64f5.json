rayItem = this.Activities.findIndex(i => i._id === id);\r\n\r\n                if (window.confirm(\"Do you really want to delete?\")) {\r\n                        axios.delete(apiURL).then(() => {\r\n                        this.Activities.splice(indexOfArrayItem, 1);\r\n                    }).catch(error => {\r\n                        console.log(error)\r\n                    });\r\n                }\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style>\r\n    .btn-success {\r\n        margin-right: 10px;\r\n    }\r\n</style>"],"sourceRoot":"src/components"}]}