/localhost:4000/user-api/register';\r\n\r\n                axios\r\n                    .post(apiURL, this.user)\r\n                    .then(() => {\r\n                        this.$router.push('/login');\r\n                        this.user = {\r\n                            name: '',\r\n                            username: '',\r\n                            email: '',\r\n                            password: '',\r\n                            confirmPassword: '',\r\n                            admin: false,\r\n                        };\r\n                    })\r\n                    .catch(error => {\r\n                        console.log(error);\r\n                    });\r\n            },\r\n        },\r\n    };\r\n</script>\r\n\r\n<style>\r\n    .required:after {\r\n        content: ' *';\r\n        color: red;\r\n    }\r\n</style>"],"sourceRoot":"src/components"}]}