ted: false,\r\n            }\r\n        },\r\n        validations: {\r\n            username: {required},\r\n            password: {required},\r\n        },\r\n        created() {\r\n            let apiURL = 'http://localhost:4000/user-api';\r\n            axios.get(apiURL).then(res => {\r\n                this.Users = res.data;\r\n            }).catch(error => {\r\n                console.log(error)\r\n            });\r\n        },\r\n        methods: {\r\n            async handleSubmitForm() {\r\n                let apiURL = 'http://localhost:4000/user-api/login';\r\n                    \r\n                this.token\r\n                 = await axios.post(apiURL, this.login).then(res => {\r\n                    if (!res.data) {\r\n                        alert(\"username or password rong\");\r\n                        this.submitted = true;\r\n                        return;\r\n                    }\r\n                    \r\n                }).catch(error => {\r\n                    console.log(\"hi there + \"+error);\r\n                });\r\n                console.log(this.token)\r\n                localStorage.setItem(\"token\", this.token);\r\n                this.$router.push(\"/calendar\");  \r\n            }\r\n        }\r\n    }\r\n</script>"],"sourceRoot":"src/components"}]}